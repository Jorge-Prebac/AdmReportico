<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : reportico.xml
    Author     : Jorge-Prebac <info@prebac.com>
    Description: Structure for the reportico table.
-->
<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>file</name>
        <type>character varying(200)</type>
    </column>
    <column>
        <name>note</name>
        <type>text</type>
    </column>
    <column>
        <name>type</name>
        <type>text</type>
    </column>
    <constraint>
        <name>reportico_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
</table>
